using System;
class HelloWorld{
static void greet()
{
    Console.WriteLine("привіт світ!");
}

static void Main()
{
    Console.WriteLine("Верба Владислав Ігорович");
}
static double sum(int a, int b)
{
    return a * b;
}
static void Main()
{
    int a = 10, int b = 67;
    Console.WriteLine(sum);
}
}